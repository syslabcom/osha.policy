<div metal:define-macro="portlet">

  <div align="center" style="border: none; margin:0px 0px 10px 0px;" tal:define="
                     banner_url string:${here/portal_url}/images/agencies.swf">
          <object style="border: none;" classid="clsid:D27CDB6E-AE6D-11cf-96B8-444553540000" codebase="http://download.macromedia.com/pub/shockwave/cabs/flash/swflash.cab#version=6,0,0,0" width="200" height="60" align="middle">
            <param name="movie"
                   value="http://europa.eu/agencies"
                   tal:attributes="value banner_url">
            <param name="quality" value="high">
            <param name="bgcolor" value="#488FCC">
            <param name="wmode" value="transparent">
            <embed style="border: none;"
                   src="http://europa.eu/agencies"
                   tal:attributes="src banner_url"
                   quality="high" bgcolor="#488FCC"
                   wmode="transparent"
                   width="200" height="60" align="middle"
                   type="application/x-shockwave-flash"
                   pluginspage="http://www.macromedia.com/go/getflashplayer"></embed> </object>

        </div>
</div>