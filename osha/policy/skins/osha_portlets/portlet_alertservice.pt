<html xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:metal="http://xml.zope.org/namespaces/metal">
<body>


<tal:block define="portlet_title string:Alert Service;
           portlet_id string:portlet_alertservice;
           col_bg_header string:#09AB11;
           col_bg_body string:#90C27E"
           metal:define-macro="portlet">
    <div class="padding" metal:use-macro="here/portlet_macro_template/macros/portletBox">
    <metal:block fill-slot="portlet_body">
      <div style="width:200px;">
        <img src="portlet_alertservice_icon.gif" style="float: right; padding: 1px 1px 1px 10px">
        <div class="portletContent" style="padding:5px; margin:0px;">
            <a href="subscribe_form" i18n:translate="portlet_alertservice_text">
Updates on topics of your choice<br>- Subscribe -</a>
        </div>
        <br clear="all">
        <div class="portletContent separated" i18n:domain="elevateIT"><strong id="numalertsubscribers">
          1001</strong> &nbsp;<span
          i18n:translate="box_oshmail_subscribers">subscribers</span>
        </div>
      </div>
<!--script language="JavaScript">new Ajax.Updater('numalertsubscribers', 'http://osha.europa.eu/showNumberAlertSubscribers', {asynchronous:true});</script-->
    </metal:block>
    </div>

</tal:block>

</body>
</html>




