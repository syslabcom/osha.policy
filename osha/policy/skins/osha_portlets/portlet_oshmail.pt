<html xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:metal="http://xml.zope.org/namespaces/metal">
<body>

<tal:block define="portlet_title string:Free Newsletter;
           portlet_id string:oshmail;
           col_bg_header string:#FFD201;
           col_bg_body string:#F2E39E"
           metal:define-macro="portlet">
<div metal:use-macro="here/portlet_macro_template/macros/portletBox">
    <metal:block fill-slot="portlet_body">

        <img src="portlet_newsletter_icon.png" style="float: right; padding: 0px 0px 0px 10px" />

        <div class="portletContent" style="padding:5px;xheight:35px; margin:0px;">
           <div>
             <a href="http://osha.europa.eu/oshmail/" title="subscribe to oshmail" i18n:attributes="title">
             <span tal:omit-tag="" i18n:translate="portlet_oshmail_text">Monthly review of strategic news<br> - Subscribe -</span></a>
           </div>
        </div>
        <br clear="all">
        <div class="portletContent separated" i18n:domain="elevateIT"><strong>
        <script type="text/javascript" src="http://osha.europa.eu/subscriptions.js">20000</script></strong><span
        i18n:translate="box_oshmail_subscribers">subscribers</span>
        </div>
    </metal:block>
</div>
</tal:block>

</body>
</html>